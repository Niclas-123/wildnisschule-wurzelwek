<!DOCTYPE html>
<html>
  <head>
    <%= render 'shared/head_tags' %>

    <%= javascript_include_tag 'https://js.stripe.com/v3/' %>
    <%= javascript_include_tag "https://www.paypal.com/sdk/js?client-id=#{Rails.application.credentials.paypal_client_id}" %>
    <%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=#{Rails.application.credentials.google_maps_key}" %>
  </head>


  <body class="frontend">
    <header>
      <%= render 'layouts/messages' %>
      <%= render 'layouts/frontend/navigation' %>
    </header>

    <main class="main-container">
      <%= yield %>
    </main>

    <footer>
      <%= render 'layouts/frontend/footer' %>
    </footer>
  </body>
</html>
